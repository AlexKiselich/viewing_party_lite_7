<%= "#{@user.user_name}'s Discover Movie Page" %>

<%= button_to "Find Top Rated Movies", user_movies_path(@user), method: :get, params: { q: "top%20rated" } %>

<%= form_with url: user_movies_path(@user), method: :get do |f| %>
  <%= f.search_field :search %>
  <%= f.submit "Find Movies" %>
<% end %>





